{
  "name": "@assessment-hrplatform/utils",
  "version": "1.0.2",
  "private": false,
  "description": "Сборник полезных утилит",
  "files": [
    "dist/"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rimraf dist",
    "build:tsc": "tsc",
    "build:assets": "cpy '**/*.{svg,png,jpg}' .. --cwd=src --parents",
    "build": "npm run clean && npm run build:tsc && npm run build:assets",
    "watch": "tsc -w",
    "lint": "eslint --ext=ts,tsx,js,jsx src",
    "lint:fix": "eslint --ext=ts,tsx,js,jsx --fix src",
    "lint-staged": "lint-staged",
    "start": "run-s watch"
  },
  "repository": "https://github.com/assessment-hrplatform/utils",
  "author": "ridrifews",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/assessment-hrplatform/utils/issues"
  },
  "homepage": "https://github.com/assessment-hrplatform/utils#readme",
  "dependencies": {
    "@hrplatform/utils": "^2.2.1",
    "@reach/router": "^1.3.4",
    "@styled-system/css": "^5.1.5",
    "lodash.isempty": "^4.4.0",
    "rc-notification": "^4.5.7",
    "redux-actions": "^2.6.5",
    "redux-mock-store": "^1.5.4",
    "styled-components": "^5.3.0",
    "react": "^17.0.2"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-typescript": "^7.14.5",
    "@types/lodash.isempty": "^4.4.6",
    "@types/reach__router": "^1.3.8",
    "@types/react": "^17.0.11",
    "@types/redux-actions": "^2.6.1",
    "@types/redux-mock-store": "^1.0.2",
    "@types/styled-components": "^5.1.10",
    "@types/styled-system__css": "^5.0.15",
    "@typescript-eslint/eslint-plugin": "~2.17.0",
    "@typescript-eslint/eslint-plugin-tslint": "~2.17.0",
    "@typescript-eslint/experimental-utils": "~2.17.0",
    "@typescript-eslint/parser": "~2.17.0",
    "cpy-cli": "^3.1.1",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.6.0",
    "eslint-plugin-functional": "^1.0.0",
    "eslint-plugin-import": "2.20.0",
    "eslint-plugin-jsdoc": "^18.0.1",
    "eslint-plugin-prettier": "^3.1.1",
    "lint-staged": "^9.2.5",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "tslint": "^5.19.0",
    "typescript": "^4.3.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged"
    }
  }
}
